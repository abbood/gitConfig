	email = abdullah.bakhach@gmail.com
[merge]
	tool = diffmerge
[github]
	user = abbood
	token = dda95951d54fc3ad08f5a20bbcec8771

[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    l = log --graph --decorate --pretty=oneline --abbrev-commit
    ll = log --pretty=format:'%h : %s' --graph --decorate
    la = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files
    rb = rebase
    com = checkout master
    rem = rebase master
    now = diff HEAD
    stache = stash
	ss = stash
	cl = "!f() { git clone git@wdca.unfuddle.com:wdca/$1.git; }; f"

[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
    
[apply]
  ignorewhitespace = change
	whitespace = nowarn
[core]
	ignorecase = false
	excludesfile = /Users/abdullahbakhach/.gitignore_global
[difftool "sourcetree"]
	cmd = diffmerge \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/diffmerge-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[diff]
	tool = diffmerge
[difftool "diffmerge"]
	cmd = diffmerge \"$LOCAL\" \"$REMOTE\"
[mergetool "diffmerge"]
	cmd = "diffmerge --merge --result=\"$MERGED\" \"$LOCAL\" \"$(if test -f \"$BASE\"; then echo \"$BASE\"; else echo \"$LOCAL\"; fi)\" \"$REMOTE\""
	trustExitCode = true
